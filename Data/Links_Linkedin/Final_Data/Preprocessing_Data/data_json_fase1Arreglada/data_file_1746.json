0 effet de serre et s\u2019adapter aux changements climatiques en agriculture.","total_emails":171,"location":{"country":"Canada","country_code":"CA","state":null,"city":"Qu\u00e9bec","timezone":"America/St_Johns","timezone_offset":"-2.5","postal_code":null,"address":"2700 rue Einstein"}},"location":{"country":"Canada","country_code":"CA","state":"Quebec","city":"Qu\u00e9bec","timezone":"America/Toronto","timezone_offset":-4.0,"postal_code":null,"raw":"Qu\u00e9bec, Quebec, Canada"}}}}